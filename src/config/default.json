{
  "compression": {},
  "express": {
    "json": {
      "inflate": true,
      "limit": "100kb",
      "reviver": null,
      "strict": true,
      "type": "application/json",
      "verify": null
    },
    "router": {
      "caseSensitive": false,
      "mergeParams": false,
      "strict": true
    }
  },
  "mongo": {
    "host": "",
    "port": "",
    "user": "",
    "password": "",
    "clientOptions": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true
    },
    "dbName": "turbulent",
    "collections": {
      "events": {
        "collectionName": "event"
      }
    }
  },
  "services": {
    "notificationBroadcaster": {
      "scheduledEvents": {
        "endpoint": "events.subscribeScheduledEvents"
      }
    }
  }
}
